#!/bin/bash
telnetvar=$(ps aux | egrep 'telnet .* [0-9]')

if [[ $telnetvar ]]
then
	printf '\e[38;5;237m \n'
	printf '\e[48;5;172m \n'
	printf '\e[K'

	if [[ -f dialupnoise.webm ]]
	then	
		vlc dialupnoise.webm
	else
		wget https://telnetnoise.s3.us-east-2.amazonaws.com/dialupnoise.webm
		vlc dialupnoise.webm
	fi
fi

