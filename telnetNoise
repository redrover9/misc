#!/bin/bash
telnetvar=$(ps aux | egrep 'telnet .* [0-9]')

if [[ $telnetvar ]]
then
	printf '\e[38;5;237m \n'
	printf '\e[48;5;172m \n'
	printf '\e[K'

	echo " _______  _______  _______  _______  _______ \n"
	echo "(  ____ \(  ____ )(  ___  )(  ____ \(  ____ \n"
	echo "| (    \/| (    )|| (   ) || (    \/| (    \/\n"
	echo "| |      | (____)|| (___) || |      | (__    \n"
	echo "| | ____ |     __)|  ___  || |      |  __)   \n"
	echo "| | \_  )| (\ (   | (   ) || |      | (      \n"
	echo "| (___) || ) \ \__| )   ( || (____/\| (____/\ \n"
	echo "(_______)|/   \__/|/     \|(_______/(_______/ \n"
	if [[ -f dialupnoise.webm ]]
	then	
		cvlc dialupnoise.webm
	else
		wget https://telnetnoise.s3.us-east-2.amazonaws.com/dialupnoise.webm
		cvlc dialupnoise.webm
	fi
fi

